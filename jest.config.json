{
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": -1
    }
  },
  "transform": {
    "^.+\\.(t|j)sx?$": "@swc/jest"
  },
  "runtime": "@side/jest-runtime",
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  }
}
