{
  "name": "nexo",
  "version": "0.1.0",
  "description": "Run javascript code from a different context",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "tsc --watch",
    "build": "npm run clean && tsc --project src",
    "clean": "rm -rf dist",
    "lint": "eslint --fix --ext .js,.ts,.json .",
    "check": "tsc --project .",
    "test": "jest",
    "test:memory": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage"
  },
  "author": {
    "name": "Pedro Gallardo",
    "email": "pedrogallardo7011@gmail.com"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@side/jest-runtime": "^1.1.0",
    "@swc/core": "^1.3.76",
    "@swc/jest": "^0.2.28",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "events": "^3.3.0",
    "is-plain-obj": "^4.1.0"
  }
}
